bhhitsplanesspsstubs +stub     :{bhhitsplanesspsstubs.scaler:%7d}
found track and ctrig          :{bhfoundtrack.scaler:%7d}
bhstublt = passed stub tests   :{bhstublt.scaler:%7d}
bh1hitslt                      :{bh1hitslt.scaler:%7d}
bh2hitslt                      :{bh2hitslt.scaler:%7d}
bh1planesgt                    :{bh1planesgt.scaler:%7d}
bh2planesgt                    :{bh2planesgt.scaler:%7d}
bhplanesgt = gt 5 planes/cham  :{bhplanesgt.scaler:%7d}
f1bhspacepoints                :{f1bhspacepoints.scaler:%7d}
f2bhspacepoints                :{f2bhspacepoints.scaler:%7d}
bhtest1=p hits/planes, f sp    :{bhtest1.scaler:%7d}
bhtest2=p sp, f stubs          :{bhtest2.scaler:%7d}
bhdid			       :{bhdid.scaler:%7d}
bhdide			       :{bhdide.scaler:%7d}
bhdidh			       :{bhdidh.scaler:%7d}
bhscinshould		       :{bhscinshould.scaler:%7d}
bhscinshoulde		       :{bhscinshoulde.scaler:%7d}
bhscinshouldh		       :{bhscinshouldh.scaler:%7d}
  COIN FID TRACK EFFIC         :{bhdid.scaler/(bhscinshould.scaler+.0001):%8.4f} +-{(sqrt(bhscinshould.scaler-bhdid.scaler)/(bhscinshould.scaler+.0001)):%8.4f}
  E COIN FID TRACK EFFIC       :{bhdide.scaler/(bhscinshoulde.scaler+.0001):%8.4f} +-{(sqrt(bhscinshoulde.scaler-bhdide.scaler)/(bhscinshoulde.scaler+.0001)):%8.4f}
  HADRON COIN FID TRACK EFFIC  :{bhdidh.scaler/(bhscinshouldh.scaler+.0001):%8.4f} +-{(sqrt(bhscinshouldh.scaler-bhdidh.scaler)/(bhscinshouldh.scaler+.0001)):%8.4f}
