*****************************
* HMS TRACKING EFFICIENCIES *
*****************************
htrig                          :{htrig.scaler:%7d}
hhitslt = passed hits/cham     :{hhitslt.scaler:%7d}
hhitsplanes = +planes >= 5/6   :{hhitsplanes.scaler:%7d}
hhitsplanessps = + sps         :{hhitsplanessps.scaler:%7d}
hhitsplanesspsstubs +stub      :{hhitsplanesspsstubs.scaler:%7d}
hsfoundtrack                   :{hsfoundtrack.scaler:%7d}
hscleantrack                   :{hscleantrack.scaler:%7d}
hstublt = passed stub tests    :{hstublt.scaler:%7d}
h1hitslt                       :{h1hitslt.scaler:%7d}
h2hitslt                       :{h2hitslt.scaler:%7d}
h1planesgt                     :{h1planesgt.scaler:%7d}
h2planesgt                     :{h2planesgt.scaler:%7d}
hplanesgt = gt 5 planes/cham   :{hplanesgt.scaler:%7d}
f1hspacepoints                 :{f1hspacepoints.scaler:%7d}
f2hspacepoints                 :{f2hspacepoints.scaler:%7d}
htest1=p hits/planes, f sp     :{htest1.scaler:%7d}
htest2=p sp, f stubs           :{htest2.scaler:%7d}
hdid			       :{hdid.scaler:%7d}
hdide			       :{hdide.scaler:%7d}
hdidh			       :{hdidh.scaler:%7d}
hscinshould		       :{hscinshould.scaler:%7d}
hscinshoulde		       :{hscinshoulde.scaler:%7d}
hscinshouldh		       :{hscinshouldh.scaler:%7d}
SING FID TRACK EFFIC           :{hdid.scaler/(hscinshould.scaler+.0001):%8.4f} +-{(sqrt(hscinshould.scaler-hdid.scaler)/(hscinshould.scaler+.0001)):%8.4f}
E SING FID TRACK EFFIC         :{hdide.scaler/(hscinshoulde.scaler+.0001):%8.4f} +-{(sqrt(hscinshoulde.scaler-hdide.scaler)/(hscinshoulde.scaler+.0001)):%8.4f}
HADRON SING FID TRACK EFFI     :{hdidh.scaler/(hscinshouldh.scaler+.0001):%8.4f} +-{(sqrt(hscinshouldh.scaler-hdidh.scaler)/(hscinshouldh.scaler+.0001)):%8.4f}
* hms coincidences...
ctrig                          :{ctrig.scaler:%7d}
bhhitslt = passed hits/cham    :{bhhitslt.scaler:%7d}
bhhitsplanes = +planes >= 5/6  :{bhhitsplanes.scaler:%7d}
