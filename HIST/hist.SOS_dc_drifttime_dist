dtime_sdc1v1,sdc_sing_drifttime(5),200,-100,300,realsdc1v1  $sdc1v1 drift time t=real1v1
dtime_sdc1v2,sdc_sing_drifttime(6),200,-100,300,realsdc1v2  $sdc1v2 drift time t=real1v2
dtime_sdc2u1,sdc_sing_drifttime(7),200,-100,300,realsdc2u1  $sdc2u1 drift time t=real2u1
